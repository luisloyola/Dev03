<div class= "morado-claro">
<br>
<br>
</div>

<div class="contain blanco">
  <br>
  <br>
<p id="notice"><%= notice %></p>
<table class="responsive-table-hoverable-centered">
    <thead>
      <tr>
        <th>Tema</th>
        <th>Tipo</th>
        <th>Discusion</th>
        <th>Accion</th>
        <th>Owner</th>
        <th>Estado</th>
        <th>Baseline</th>
        <th colspan="3"></th>
      </tr>
    </thead>
  <tbody>
        <% @tasks.each do |reunion| %>
        <tr>
          
          <td><%= reunion.tema %></td>
          <td><%= reunion.tipo %></td>
          <td><%= reunion.discusion %></td>
          <td><%= reunion.accion %></td>
          <td><%= reunion.owner %></td>
          <td><%= reunion.estado %></td>
          <td><%= reunion.baseline %></td>
        </tr>
        <% end %>
  </tbody>
</table>

<%= link_to 'Volver', reunions_path %>

</div>







